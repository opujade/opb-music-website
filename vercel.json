{
	"version": 2,
	"builds": [
		{
			"src": "api/images.js",
			"use": "@vercel/node"
		},
		{
			"src": "frontend/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "frontend/dist"
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/images.js"
		},
		{
			"src": "/images/(.*)",
			"dest": "/images/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/frontend/$1"
		}
	]
}
